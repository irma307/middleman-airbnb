
<ul class="list-inline text-center">
  <% data.users.each do |user| %>
  <li>
    <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.github_nickname}", class: "img-circle", height: "100" %>
    <p><strong><%= user.name %></strong></p>
    <p><em><%=user.github_nickname %></em></p>
  </li>
  <% end %>
</ul>
